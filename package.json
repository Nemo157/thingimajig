{
  "name": "thingimajig",
  "version": "1.0.0",
  "description": "",
  "main": "server/main.js",
  "scripts": {
    "browserify:main": "browserify --debug -t babelify -o client/main.js src/client/main.es6",
    "browserify:renderer": "browserify --debug -t babelify -o client/renderer.js src/client/renderer.es6",
    "browserify": "npm run browserify:main && npm run browserify:renderer",
    "watchify:main": "watchify --debug -t babelify -o client/main.js src/client/main.es6",
    "watchify:renderer": "watchify --debug -t babelify -o client/renderer.js src/client/renderer.es6",
    "watchify": "npm run watchify:main && npm run watchify:renderer",
    "cp": "mkdir -p client && cp static/* client",
    "build": "npm run cp && npm run browserify",
    "start": "node server/main.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babelify": "^6.0.2",
    "browserify": "^9.0.3"
  },
  "dependencies": {
    "babel": "^5.1.13",
    "knockout": "^3.3.0",
    "socket.io": "^1.3.5",
    "virtual-dom": "^2.0.1"
  }
}
